				</hiddenSegments>
			</requestFiltering>
		</security><handlers>
			<remove name="ReactJsx" />
			<add name="ReactJsx" verb="GET" path="*.jsx" type="React.Web.JsxHandlerFactory, React.Web" preCondition="integratedMode" />
		</handlers></system.webServer>
