@model IEnumerable<Rebusjakt.Models.Hunt>
    @{
        ViewBag.Title = "Sök jakter";
    }
@section Head{
    <style type="text/css">
        .form-help { margin:0; }
    </style>
}

<div class="content-heading">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-lg-push-3 col-sm-10 col-sm-push-1">
                <h1 class="heading">Sök jakter</h1>
            </div>
        </div>
    </div>
</div>
<div class="content-inner">
    <div class="container">
        @Html.React("SearchApp", new { 
            data = Model,
            uselocation = ViewBag.UseLocation != null,
            query = ViewBag.Query 
        })
    </div>
</div>
@section Scripts{
    @Scripts.Render("~/bundles/search")
    @Html.ReactInitJavaScript()
}

